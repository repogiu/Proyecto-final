//Animacion encargada de dar un informe de sistema de ERROR GRAVE, el cual necesita ser verificado por "Guardia()"
//posteriormente. Tambien se utilizaria en el momento que Guardia() realiza la verificación. 
SubProceso SubAnimacionError()
	Definir i Como Entero;
	Limpiar Pantalla;
	TituloSistema();
	Esperar 400 Milisegundos;
	Limpiar Pantalla;
	TituloSistema();
	Escribir " 																		ERROR ERROR";
	Escribir " 																  	    ERROR ERROR";
	Esperar 500 Milisegundos;
	Limpiar Pantalla;
	TituloSistema();
	Escribir " 														    ERROR ERROR ERROR ERROR ERROR ERROR ";
	Escribir " 															ERROR ERROR ERROR ERROR ERROR ERROR ";
	Esperar 400 Milisegundos;
	Limpiar Pantalla;
	TituloSistema();
	Escribir " 													  ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ";
	Escribir " 													  ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ";
	Esperar 300 Milisegundos;
	Limpiar Pantalla;
	TituloSistema();
	Esperar 300 Milisegundos;
	Escribir " 										  ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR";
	Escribir " 										  ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR";
	Esperar 300 Milisegundos;
	Limpiar Pantalla;
	TituloSistema();
	Esperar 300 Milisegundos;
	Escribir " 								    ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR";
	Escribir " 								    ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR";
	Esperar 500 Milisegundos;
	Limpiar Pantalla;
	TituloSistema();
	Esperar 300 Milisegundos;
	Escribir " 					    ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR";
	Escribir " 					    ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR";
	Esperar 500 Milisegundos;
	Limpiar Pantalla;
	TituloSistema();
	Esperar 300 Milisegundos;
	Escribir " 								    ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR";
	Escribir " 								    ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR";
	Esperar 600 Milisegundos;
	Limpiar Pantalla;
	TituloSistema();
	Esperar 100 Milisegundos;
	Escribir " 										  ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR";
	Escribir " 										  ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR";
	Esperar 100 Milisegundos;
	Limpiar Pantalla;
	TituloSistema();
	Esperar 100 Milisegundos;
	Escribir " 													  ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ";
	Escribir " 													  ERROR ERROR ERROR ERROR ERROR ERROR ERROR ERROR ";
	Esperar 100 Milisegundos;
	Limpiar Pantalla;
	TituloSistema();
	Esperar 100 Milisegundos;
	Escribir " 														    ERROR ERROR ERROR ERROR ERROR ERROR ";
	Escribir " 															ERROR ERROR ERROR ERROR ERROR ERROR ";
	Esperar 100 Milisegundos;
	Limpiar Pantalla;
	TituloSistema();
	Esperar 100 Milisegundos;
	Escribir " 														          ERROR ERROR ERROR ERROR  ";
	Escribir " 															      ERROR ERROR ERROR ERROR ";
	Esperar 100 Milisegundos;
	Limpiar Pantalla;
	TituloSistema();
	Esperar 100 Milisegundos;
	Escribir " 																		ERROR ERROR";
	Escribir " 																  	    ERROR ERROR";
	Esperar 100 Milisegundos;
	Limpiar Pantalla;
	TituloSistema();
	Esperar 100 Milisegundos;
	Escribir "                                                                            ERROR";
	Escribir "                                                                            ERROR";
	Esperar 100 Milisegundos;
	Limpiar Pantalla;
FinSubProceso

//Animacion que se llama desde "SeAbreBarrera()" si se encuentra un error (por medio de Azar), es la encargada 
//de notificar al usuario, y también es la encargada de redireccionar hacia "Guardia()"
SubProceso animacionDeBarreraRedirigiendoAGuardia()
	Definir i Como Entero;
	Limpiar Pantalla;
	SubAnimacionError();
	Esperar 700 Milisegundos;
	Para i<-0 Hasta 2 Con Paso 1 Hacer
		Limpiar Pantalla;
		TituloSistema();
		Escribir "		Detectamos un ERROR en la solicitud."; 
		Escribir "		Aguarde un momento, un Guardia verificará el inconveninte.";
		Escribir " ";
		Escribir "		Solicitando";
		Esperar 600 Milisegundos;
		Limpiar Pantalla;
		TituloSistema();
		Escribir "		Detectamos un ERROR en la solicitud."; 
		Escribir "		Aguarde un momento, un Guardia verificará el inconveninte.";
		Escribir " ";
		Escribir "		Solicitando.";
		Esperar 600 Milisegundos;
		Limpiar Pantalla;
		TituloSistema();
		Escribir "		Detectamos un ERROR en la solicitud."; 
		Escribir "		Aguarde un momento, un Guardia verificará el inconveninte.";
		Escribir " ";
		Escribir "		Solicitando..";
		Esperar 600 Milisegundos;
		Limpiar Pantalla;
		TituloSistema();
		Escribir "		Detectamos un ERROR en la solicitud."; 
		Escribir "		Aguarde un momento, un Guardia verificará el inconveninte.";
		Escribir " ";
		Escribir "		Solicitando...";
		Esperar 600 Milisegundos;
	FinPara
	Limpiar Pantalla;
	Guardia();
FinSubProceso

// Animacion para simular reconexion de barrera desde "Guardia()", la misma tambien da cierre aal programa.
SubProceso animacionDeBarrera ( )
	Definir i Como Entero;
	Esperar 400 Milisegundos;
	SubAnimacionError();
	Esperar 1200 Milisegundos;
	Para i<-0 Hasta 2 Con Paso 1 Hacer
		Limpiar Pantalla;
		TituloSistema();
		TituloSubAnimacionDeBarrera();
		Escribir "		Restableciendo conexión";
		Esperar 400 Milisegundos;
		Limpiar Pantalla;
		TituloSistema();
		TituloSubAnimacionDeBarrera();
		Escribir "		Restableciendo conexión";
		Esperar 400 Milisegundos;
		Limpiar Pantalla;
		TituloSistema();
		TituloSubAnimacionDeBarrera();
		Escribir "		Restableciendo conexión..";
		Esperar 400 Milisegundos;
		Limpiar Pantalla;
		TituloSistema();
		TituloSubAnimacionDeBarrera();
		Escribir "		Restableciendo conexión...";
		Esperar 400 Milisegundos;
	FinPara
	Limpiar Pantalla;
	TituloSistema();
	Esperar 200 Milisegundos;
	Escribir "                                                                  ¡LA BARRERA SE HA ARREGLADO!";
	Esperar 200 Milisegundos;
	Escribir "";
	Escribir "";
	Escribir "                                                                        [ Abrir Barrera ]";
	Escribir ""; 
	Escribir"                                                                     Presione cualquier tecla.." Sin Saltar ;
	Esperar Tecla;
	SubAnimacionDespedida();
FinSubProceso

// SubaAnimacion para ser llamada desde la "animacionDeBarrera()"
SubProceso TituloSubAnimacionDeBarrera()
	Escribir "		Se ha detectado que el error es debido a un problema de conexión:"; 
	Escribir " ";
FinSubProceso

//Animacion para simulacion de barrera en estado de OK y despedida para clientes. 
SubProceso SubAnimacionDespedida()
	Definir i Como Entero;
	Esperar 1500 Milisegundos;
	Para i<-0 Hasta 2 Con Paso 1 Hacer
		Limpiar Pantalla;
		TituloATC();
		Escribir "                                                                                                          ";
		Escribir "                                                                                                          ";
		Escribir "                                                                                                          ";
		Escribir "                                                                                                          ";
		Escribir "                                                                             ===============              ";
		Escribir "                                                                                          / \\            ";
		Escribir "                                                                                         ||  ||           ";
		Escribir "                                                                 *Se abre la barrera*    ||  ||           ";
		Esperar 300 Milisegundos;
		Limpiar Pantalla;
		TituloATC();
		Escribir "                                                                                      \\                  ";
		Escribir "                                                                                       \\                 ";
		Escribir "                                                                                        \\                ";
		Escribir "                                                                                         \\               ";
		Escribir "                                                                                          \\              ";
		Escribir "                                                                                          / \\            ";
		Escribir "                                                                                         ||  ||           ";
		Escribir "                                                                 *Se abre la barrera*    ||  ||           ";
		Esperar 400 Milisegundos;
		Limpiar Pantalla;
		TituloATC();
		Escribir "                                                                                                          ";
		Escribir "                                                                                                          ";
		Escribir "                                                                                                          ";
		Escribir "                                                                                                          ";
		Escribir "                                                                             ===============              ";
		Escribir "                                                                                          / \\            ";
		Escribir "                                                                                         ||  ||           ";
		Escribir "                                                                 *Se abre la barrera*    ||  ||           ";
		Esperar 300 Milisegundos;
		Limpiar Pantalla;
		TituloATC();
		Escribir "                                                                                      \\                  ";
		Escribir "                                                                                       \\                 ";
		Escribir "                                                                                        \\                ";
		Escribir "                                                                                         \\               ";
		Escribir "                                                                                          \\              ";
		Escribir "                                                                                          / \\            ";
		Escribir "                                                                                         ||  ||           ";
		Escribir "                                                                 *Se abre la barrera*    ||  ||           ";
		Esperar 400 Milisegundos;
	FinPara
	Esperar 200 Milisegundos;
	Escribir "";
	Escribir " ";
	Escribir " ";
	Escribir "                                                              Gracias por su visita en Hamster Mall.";
	Esperar 200 Milisegundos;
FinSubProceso



