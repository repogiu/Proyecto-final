//Servicio de Parking******************************************************************************************************************************************
//MAIN - INICIALIZADOR*****************************************************************************************************************************************
Proceso Parking
	Limpiar Pantalla;
	TituloATC();
	Estructura();
FinProceso

// NUEVO************************************************************************************************************


//Desde aqui vamos a relizar las principales funciones, tambien econtraremos la matriz, Tendremos el menu principal (ATC)
//y haremos las llamadas a los correspondientes subMenus. 
SubProceso Estructura()
	//Matriz "Base de Datos"
	definir limite, i,j,k,c, entrada,caja, ingresados, p, abonados,total,recaudado,conteo,opcion Como Entero;
	Definir  bandera, bandera2, bandera3 Como Logico;
	Definir ocompra Como Caracter;
	limite<-5; //tamaño del vector
	i<-0;       //iterador que guarda los registros CA
	c<-0;       //iterador que guarda los registros CNR
	bandera<-Falso; //indica si desea continuar o no en el menu dentro del ciclo repetir
	bandera2<-falso; //verifica si el registro ya existe
	bandera3<-falso; //verifica si el usuario abono cuota mensual
	entrada<-0;  //captura la opcion ingresada
	caja<-0;    //captura la opcion ingresada en menu recaudacion
	ingresados<-0; //variable que almacena los CA
	conteo<-0; //variable que almacena CNR
	opcion<-0; //captura la opcion cnr
	j<-0; //iterador para busqueda
	k<-0;
	p<-0; //iterador que verifica si ya existe el registro o no
	abonados<-0; //iterador que cuenta CA
	ocompra<-"";
	total<-0; //monto recaudado total
	recaudado<-0; //monto recaudado de CNR
	
	Dimension nombre[5];
	Dimension apellido[5];
	Dimension dni[5];
	dimension abono[5];
	dimension sumaabono[5];
	definir nombre, apellido, dni, busqueda como texto;
	definir monto, abono,costo, recaudoAbono,sumaabono como real;
	busqueda<-"";
	
	//antes de entrar a la estructura repetir le asigamos valores nulos a las variables
	para k<-0 hasta limite-1 con paso 1 hacer
		dni[k]<-"";
		nombre[k]<-"";
		apellido[k]<-"";
		abono[k]<-0;
	FinPara
	Repetir
		Limpiar Pantalla;
		// Menu principal, donde registraremos Usuario en la Matriz, Ingresaremos a un sub menu para dar ingreso al servicio 
		//de parking a los distintos Cleintes, Podremos listar el Registro de Usuario y solicitar el Arqueo de Caja.
		TituloATC();
		Escribir "		MENU:";
		Escribir "		1. Registrar Nuevo Usuario";
		Escribir "		2. Ingresar Usuario";
		Escribir "		3. Registro de Usuarios";
		Escribir "		4. Arqueo de caja";
		Escribir ' ';
		Escribir '		Ingrese la opción deseada: ' Sin Saltar;
		Leer entrada;
		Limpiar Pantalla;
		TituloATC();
		Segun entrada hacer
			//Registrar usuarios como Cliente Abonado, los mismos se iran guardando en la matriz, con un numero identificador autoasignado
			1: escribir "		REGISTRAR USUARIO";
				i<-ingresados;
				bandera2<-Falso; //Se reinicia la variable a cada vuelta
				si i<>limite entonces
					Escribir Sin Saltar"		Nombre: ";
					Leer nombre[i];
					Escribir Sin Saltar "		Apellido: ";
					Leer apellido[i];
					Escribir Sin Saltar "		DNI: ";
					Leer dni[i];
					mientras Longitud(dni[i]) <> 8 hacer
						Escribir "		El DNI debe tener 8 digitos";
						Escribir "		Digite su DNI";
						Leer dni[i];
					FinMientras
					escribir sin saltar "		Abono mensual: ";
					leer abono[i];
					para p<-0 hasta limite-1 con paso 1 hacer
						si dni[p]=dni[i] y i<>p Entonces //Se verifica si el dni ingresado existe, teniendo en cuenta que no compara las mismas posiciones.
							bandera2<-verdadero;
							p<-limite; //para reinicializar y no seguir buscando
						FinSi
					FinPara
					si bandera2 Entonces
						nombre[i]<-"";
						apellido[i]<-"";
						dni[i]<-"";
						abono[i]<-0;
						Escribir "		Este registro ya existe";
					SiNo
						limpiar pantalla;
						TituloATC();
						VerificacionDeDatos();
						Limpiar Pantalla;
						TituloATC();
						i<-i+1;
						Escribir "		¡Registro Exitoso!";
						ingresados<-ingresados+1; //para que me ingrese el registro a la bbdd
						si abono[i]=3000 Entonces
							abonados<-abonados+1;
						FinSi
					FinSi
				SiNo
					Limpiar Pantalla;
					TituloATC();
					Escribir "		Ya llego al limite";
				FinSi
			2: //ingresar al parking
				//Desde este menu podemos gestionar los Distintos tipos de Clientes, Abonados, con Orden de Compra y CNR.
				Limpiar Pantalla;
				TituloATC();
				Escribir "		MENU: ";
				Escribir '		1. Cliente Abonado';
				Escribir '		2. Cliente con Orden de Compra';
				Escribir '		3. Cliente No Registrado';
				Escribir '		4. Salir';
				Escribir ' ';
				Escribir '		Ingrese la opción deseada: ' Sin Saltar;
				leer entrada;
				Limpiar Pantalla;
				segun entrada hacer
					1://Por medio de una consulta a la matriz, traemos el registro con mismo n de DNI
						si ingresados > 0 entonces
							TituloATC();
							Escribir sin saltar "		Digite su DNI: ";
							Leer busqueda;
							Limpiar Pantalla;
							TituloATC();
							Esperar 500 Milisegundos;
							para j<-0 hasta limite-1 con paso 1 hacer
								si dni[j]==busqueda entonces
									Limpiar Pantalla;
									TituloATC();
									Escribir "		Registro encontrado:";
									Escribir "";
									Escribir "----------------------------------------------------------------------------------------------------------------------------------------------------------------";
									escribir "		ID: ",j+1," | Nombre: ",nombre[j]," | Apellido: ",apellido[j]," | DNI: ",dni[j]," | Abono mensual: ",abono[j], ;
									Escribir "----------------------------------------------------------------------------------------------------------------------------------------------------------------";
									Esperar 2 segundos;
									Escribir "";
									Escribir "";
									Escribir "                                                                        [ Abrir Barrera ]";
									Escribir ""; 
									Escribir "                                                                    Presione cualquier tecla.."Sin Saltar ;
									Esperar Tecla;
									Esperar 700 Milisegundos;
									Limpiar Pantalla;
									seAbreBarrera();		
									Escribir "";
									Escribir "                                                                ¿Desea realizar otra operación?";               
									Escribir "                                                                    1. SI             2. NO";
									Escribir ' ';
									Escribir '		Ingrese la opción deseada: ' Sin Saltar;
									Leer entrada;
									si entrada==1 Entonces
										bandera<-Verdadero;
										Limpiar Pantalla;
									SiNo
										bandera<-falso;
										Limpiar Pantalla;
										TituloATC();
										Escribir "		Hasta pronto...";
										Creditos();
									FinSi
								SiNo														//SE AGREGO EL SINO PARA DATOS DEL DNI ERRONEO
									Limpiar Pantalla;
									VerificacionDeDatos();
									Escribir "		DNI Erróneo, intente nuevamente";
								FinSi
							j<-limite; // cuando ya encuentre el registro se detenga y no siga buscando    //SE CORRIO EL STOP		
							FinPara	
						SiNo
							Limpiar Pantalla;
							TituloATC();
							VerificacionDeDatos();
							Limpiar Pantalla;
							TituloATC();
							Escribir "		No se han encontrado registros...";
						FinSi
					2:	// Asignamos a un mismo vector distintos numeros de Ordenes de Compra, para ser validados posteriormente
						//Definir OrdenCompra1, OrdenCompra2, OrdenCompra3 Como Caracter;
						Definir OrdenCompra Como Caracter;;
						Dimension OrdenCompra[3];
						OrdenCompra[0]<-'1111';
						OrdenCompra[1]<-'2222';
						OrdenCompra[2]<-'3333';
						Limpiar Pantalla;
						TituloATC();
						escribir "		Digite el número de Orden de Compra: ";                              
						leer ocompra;											
						mientras Longitud(ocompra) <> 4 hacer
							Escribir "		El numero de compra debe tener 4 digitos";
							Escribir ' ';
							Escribir '		Ingrese el número de Orden de Compra: ' Sin Saltar;
							Leer ocompra;
						FinMientras
						si ocompra = OrdenCompra[0] O ocompra = OrdenCompra[1] O ocompra = OrdenCompra[2] Entonces
							VerificacionDeDatos();
							Limpiar Pantalla;
							TituloATC();
							Escribir "";
							Escribir "		                                                                   * Orden Validada * ";
							Escribir "		                                                         ¡Gracias por comprar por nuestra WEB!";
							Escribir "";
							Escribir "";
							Escribir "                                                                        [ Abrir Barrera ]";
							Escribir ""; 
							Escribir "                                                                    Presione cualquier tecla.."Sin Saltar ;
							Esperar Tecla; 
						FinSi
						Esperar 700 Milisegundos;
						Limpiar Pantalla;
						seAbreBarrera();
					3:	//CNR
						c<-conteo;
						c<-c+1;
						conteo<-conteo+1;
						CRN();
					4: //Salir
				FinSegun
			3: //Listar, Realiamos una lista de todos los registros en nuestra matriz (tener en cuenta que los mismos se borraran una vez 
				// finalizado el programa. 
				i<-0; //reiniciamos la variable i para que me traiga los datos desde la posicion inicial
				si ingresados > 0 Entonces
					para i<-0 hasta limite-1 con paso 1 hacer
						si nombre[i]<>"" Entonces
							Escribir "----------------------------------------------------------------------------------------------------------------------------------------------------------------";
							escribir " 	ID: ",i+1," | Nombre: ",nombre[i]," | Apellido: ",apellido[i]," | DNI: ",dni[i]," | Abono mensual: ",abono[i];
							Escribir "----------------------------------------------------------------------------------------------------------------------------------------------------------------";
							Escribir "";
						FinSi
					FinPara
					para i<-0 hasta limite-1 con paso 1 hacer
						
					FinPara
					para i<-0 hasta limite-1 con paso 1 hacer
						si abono[i]<>0 entonces
							sumaabono[i]<-abono[0]+abono[1]+abono[2]+abono[3]+abono[4];
							//escribir"		Subtotal: ",sumaabono[i];
							//limite = limite + 1;
						FinSi
					FinPara
				SiNo
					Escribir "		No hay registros en el sistema";
				FinSi
			4: // En este menu podemos ir registrando el mongo en nuestro arqueo de caja, se suman los CNR y dan conteo, clientes Abonados (registrados)
				// con exepcion de clientes que ingresan con OC.
				i<-0;
				Escribir "		MENU RECAUDACIÓN";
				si ingresados>0 Entonces
					sumaabono[i]<-abono[0]+abono[1]+abono[2]+abono[3]+abono[4];
					monto<-0;
					abonados<-abonados+1;
					monto<-sumaabono[i];
					escribir sin saltar "		El monto recaudado de clientes abonados es: ",monto;
					Escribir "";
					//escribir sin saltar "Cantidad de clientes abonados es: ",abonados;
				SiNo
					monto<-0;
					Escribir "		El monto recaudado de clientes abonados es: ",monto;
				FinSi
				Escribir "";
				
				si conteo>0 Entonces
					costo <- 0;
					recaudado<-0;
					//conteo<-conteo+1;
					costo<-150;
					recaudado<-costo*conteo;
					escribir "";
					Escribir Sin Saltar "		El monto recaudado de Clientes no Registrados es: ",recaudado;
					escribir "";
					escribir sin saltar "		Cantidad de Clientes no Registrados es: ",conteo;
				sino
					recaudado<-0;
					Escribir "		El monto recaudado de Clientes no Registrados es: ",recaudado;
				FinSi
				escribir"";
				total<-monto+recaudado;
				escribir Sin Saltar "		El monto recaudado total es: ",total;
				Escribir "";
				Escribir "";
				
			De Otro Modo:
				Escribir "		Opción no valida";
		FinSegun
		//Metodo para consultar si se desea seguir gestionando en la app.
		Escribir "";
		Escribir "                                                                ¿Desea realizar otra operación?";               
		Escribir "                                                                    1. SI             2. NO";
		Escribir ' ';
		Escribir '		Ingrese la opción deseada: ' Sin Saltar;
		Leer entrada;
		si entrada==1 Entonces
			bandera<-Verdadero;
			Limpiar Pantalla;
		SiNo
			bandera<-falso;
			Limpiar Pantalla;
			TituloATC();
			Escribir "		Hasta pronto...";
			Creditos();
		FinSi
	Hasta Que bandera==Falso; //se sale del sistema
FinSubProceso


//ABRIR o CERRAR la BARRERA**********************************************************************************************************************************************

// Metodo encargado de la apertura de la barrera, el mismo es el que decide si la barrera da error o no por medio de Azar.
//Tambien es el encargado de redirigirnos o a la guardia o directamente a la apertura de la barrera.
SubProceso SeAbreBarrera ()
	Definir barreraRota, i Como entero;
	Limpiar Pantalla;
	TituloATC();
	Escribir " ";
	barreraRota <- Azar(2);
	Si barreraRota = 0 Entonces //¿la barrera está rota? *abajo del todo, hay una idea sobre esto
		ASolicitarBarrera();
		Limpiar Pantalla;
		TituloATC();
		SubAnimacionDespedida();
	SiNo
		ASolicitarBarrera();
		animacionDeBarreraRedirigiendoAGuardia();
	FinSi
FinSubProceso


//ATC - 1ra PARTE - 2da PARTE ****************************************************************************************************************************************

// Menu verificador de ticket de cliente.
SubProceso MenuATC()
	definir opcion,nroTicket como entero;
	//definir ticket,monto como real;
	//Dimension ticket[3,2];
	definir i,j como entero;
	nroTicket <- 0;
	TituloATC();
	escribir Sin Saltar "		Ingrese el número de ticket otorgado al Cliente: ";
	leer nroTicket;
	//Verificamos que el número de Ticket sea el correcto, este while se repetira hasta que se
	//valide como correcto.
	Mientras nroTicket<>1234 hacer
		Limpiar Pantalla;
		TituloATC();
		VerificacionDeDatos();
		Limpiar Pantalla;
		TituloATC();
		escribir "                                                                 ¡Número de ticket incorrecto!";
		Escribir "";
		escribir Sin Saltar "		Ingrese el número de ticket nuevamente: ";
		leer nroTicket;
	FinMientras
	Limpiar Pantalla;
	TituloATC();
	Escribir "		MENU: ";
	Escribir " ";
	Escribir "		1. Efectuar cobro";
	Escribir "		2. Salir";
	Escribir " ";
	Escribir Sin Saltar "		Ingrese la opción deseada: ";
	leer opcion;
	Escribir "";
	segun opcion Hacer
		1:
			//Llama a un procedimiento para realizar el cobro
			animacionFormasDePago();
			menuCobro();
		2: 
		De Otro Modo:
			TituloATC();
			Escribir "		Hasta pronto..";
	FinSegun	
FinSubProceso

// Sub Menu para tipo de cobro para cliente CNR, con un Segun de dos opciones, Cobro al contado y Cobro por debito (cobro por tarjeta valida n tarjeta
// y calve de seguridad. 
SubProceso menuCobro ()
	Definir efectivo,opcion  Como Entero;
	definir numTarjeta,clave Como Caracter;
	Limpiar Pantalla;
	TituloATC();
	Escribir '		Ingrese un modo de pago: ';
	Escribir " ";
	Escribir '		1. Pago de contado';
	Escribir '		2. Pago con tarjeta';
	Escribir '		3. Salir';
	Escribir " ";
	Escribir Sin Saltar "		Ingrese la opción deseada: ";
	leer opcion;
	Escribir " ";
	Segun opcion  Hacer
		1:  
			Limpiar Pantalla;
			TituloATC();
			Escribir , "		Valor del TICKET $150";
			Escribir Sin Saltar, "		Ingrese el monto a cobrar: ";
			leer efectivo;
			//solo acepta el importe exacto si pone otro tipos de billetes devuelve el dinero y empieza de nuevo el bucle
			precesandoPago();
			Escribir "";
			Escribir "";
			Escribir "                                                                        [ Abrir Barrera ]";
			Escribir ""; 
			Escribir "                                                                    Presione cualquier tecla.."Sin Saltar ;
			Esperar Tecla; 
			SeAbreBarrera();
		2: //estaria bueno conertir los numeros en asteriscos
			Escribir Sin Saltar"		Ingrese el número de tarjeta: ";
			leer numTarjeta;
			mientras Longitud(numTarjeta) <> 16 hacer
				Escribir "		El número de tarjeta debe tener 16 digitos";
				Escribir Sin Saltar"		Ingrese el número de tarjeta del cliente";
				leer numTarjeta;
			FinMientras
			Escribir "";
			escribir Sin Saltar"		Ingrese la clave de seguridad: ";
			leer clave;
			mientras Longitud(clave) <> 3 hacer
				Escribir "		La clave debe tener 3 digitos";
				escribir Sin Saltar"		Ingrese la clave de seguridad: ";
				leer clave;
			FinMientras
			precesandoPago();
			Escribir "";
			Escribir "";
			Escribir "                                                                        [ Abrir Barrera ]";
			Escribir ""; 
			Escribir"                                                                     Presione cualquier tecla.." Sin Saltar ;
			Esperar Tecla;
			SeAbreBarrera();
		3: //Salir
		De Otro Modo:
			Escribir "		La opción ingresada no es válida, intente nuevamente";
	FinSegun
	Escribir "";
FinSubProceso

//GUARDIA*************************************************************************************************************************************************************

//Guardia recibe a modo de notificacion un solicitud para la verificacion de un error en la barrera, la misma la 
//redirige la funcion Azar que se enuentra en el metodo "SeAbreBarrera()". Una vez que se da ok a verificar, esta 
//simula una verificacion y reconexion por medio la animacion de "animacionDeBarrera()", luego la cual,
//finaliza el ejecucion del programa. 

SubProceso Guardia ()
	TituloGuardia();
	Escribir "                                                  NOTIFICACIÓN: Se solicita una Verificación de Barrera";
	Escribir "";
	Escribir "";
	Escribir "";
	Escribir "";
	Escribir "                                                                        [ Verificar ]";
	Escribir ""; 
	Escribir "                                                                  Presione cualquier tecla..                            " Sin Saltar ;
	Esperar Tecla;
	animacionDeBarrera();
FinSubProceso

// MENU CNR *******************************************************************************************************************************
// Para los Clientes no registrados, desde esta interfaz se les informa el valor de la estadia, y se le otorga un numero para su validacion
// el mismo numero por el momento solo sera "1234". este mismo numero se validara una vez que el cliente decida "Pagar Ticket", lo
// cual lo rediririgira hacia "menuATC()". La segunda opcion simplemente finaliza la ejecucion dando una despedida.
SubProceso CRN()
	Definir opcion Como Entero;
	TituloATC();
	Escribir "                                                            #########################################                      ";
	Escribir "                                                            #   El servicio de parking cuesta $150  #                      ";
	Escribir "                                                            #                                       #                      ";
	Escribir "                                                            #   El número de Ticket es el * 1234 *  #                      ";
	Escribir "                                                            #########################################                      ";
	Escribir " ";
	Escribir "		MENU: ";
	Escribir " ";
	Escribir "		1. ACEPTAR";
	Escribir "		2. CANCELAR";
	Escribir " ";
	Escribir Sin Saltar "		Ingrese la opción deseada: ";
	leer opcion;
	Segun opcion Hacer
		1:
			Esperar 400 Milisegundos;
			Limpiar Pantalla;
			MenuATC;
			Esperar 3 segundos;
		2:
			Limpiar Pantalla;
			Esperar 400 Milisegundos;
			TituloATC();
			Escribir " ";
			Escribir "                                               LAMENTAMOS QUE NO PUEDA QUEDARSE, LOS ESPERAMOS EN OTRA OPORTUNIDAD.";
			//Aqui no utilizamos SubAnimacionDespedida(), porque epara este evento no se necesitaria abrir barrera.
			Esperar 1500 Milisegundos;
			Escribir " ";
			Escribir " ";
			Escribir " ";
			Escribir "                                                             Gracias por su visita en Hamster Mall.";
			Esperar 2000 Milisegundos;
	FinSegun
FinSubProceso

// Pausa la ejecucion del programa (sin finalizarlo),hasta que se presione alguna tecla, una vez presionada, se reanudaria la ejecucion.
SubProceso Pausa()
	Escribir " ";
	Esperar Tecla;
FinSubProceso

//ANIMACIONES***********************************************************************************************************************
//Estaran encargadas de darle mas Realizmo a la aplicacion, tambien encargadas de ahorranos lineas de codigo, ya que por ejemplo
//los "titulos" los utilizamos una y otra vez, los creariamos una sola vez como SubProceso y los llamariamos constantemente 
//donde se los requiera.
//La mayor parte dan un valor visual, otros contendran otras animaciones internarnas y algunas encargadas de redirigirnos hacia otros 
//menus.

// Titulos para las diferentes ventanas por las cuales se navegara TituloCreditos(), TituloATC(), TituloGuardia() y TituloSistema()
//Animacion de tipo visual
SubProceso TituloCreditos()
	Escribir "-------------------------------------------------------->>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>--------------------------------------------------------";
	Escribir "                                                                    TEAM INFORMATIC HAMSTERS                                                                    ";
	Escribir "                                                                    ------------------------                                                                    ";
	Escribir "--------------------------------------------------------<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<--------------------------------------------------------";
	Escribir " ";
FinSubProceso

//Animacion de tipo visual
SubProceso TituloATC()
	Escribir "-------------------------------------------------------->>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>--------------------------------------------------------";
	Escribir "                                                               ATENCION AL CLIENTE DE HAMSTER MALL                                                              ";
	Escribir "                                                               -----------------------------------                                                              ";
	Escribir "--------------------------------------------------------<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<--------------------------------------------------------";
	Escribir " ";
	Escribir " ";
FinSubProceso

//Animacion de tipo visual
SubProceso TituloGuardia()
	Escribir "-------------------------------------------------------->>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>--------------------------------------------------------";
	Escribir "                                                               SERVICIO DE GUARDIA DE HAMSTER MALL                                                              ";
	Escribir "                                                               -----------------------------------                                                              ";
	Escribir "--------------------------------------------------------<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<--------------------------------------------------------";
	Escribir " ";
FinSubProceso

//Animacion de tipo visual
SubProceso TituloSistema()
	Escribir "-------------------------------------------------------->>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>--------------------------------------------------------";
	Escribir "                                                                  ¡¡ INFORMACION DE SISTEMA !!                                                                  ";
	Escribir "                                                                  ----------------------------                                                                  ";
	Escribir "--------------------------------------------------------<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<--------------------------------------------------------";
	Escribir " ";
FinSubProceso

//Animacion de tipo visual
SubProceso animacionFormasDePago ( )
	Definir i Como Entero;
	Esperar 800 Milisegundos;
	Para i<-0 Hasta 2 Con Paso 1 Hacer
		Limpiar Pantalla;
		TituloATC();
		Escribir "		Cargando formas de pago, aguarde";
		Esperar 400 Milisegundos;
		Limpiar Pantalla;
